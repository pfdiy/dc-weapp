/* pages/scan/index.wxss */
.container {
  width: 100%;
  height: 100vh;
}
.map-view {
  width: 100%;
  height: 100vh;
}
.map-button {
  margin-top: 80vh;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  z-index: 100;
}
.map-button1 {
  width: 210rpx;
  height: 110rpx;
  position: absolute;
  z-index: -2;
  border-radius: 60rpx;
  background-color: #ffffff;
  opacity: 0.7;
}
.sf {
  display: flex;
  margin-top: 150rpx;
  justify-content: center;
  align-items: center;
  width: 100%;
  position: absolute;
  z-index: 1;
}
.sf1 {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  width: 700rpx;
  height: 300rpx;
  background-color: #ffffff;
  border-radius: 20rpx;
  z-index: 1;
}
.sf2 {
  width: 400rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.sf3 {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.vertical {
  display: inline-block;
  width: 2rpx;
  background-color: #dcdfe6;
  margin: 0 16rpx;
}

.sf3-text1 {
  font-size: 22rpx;
}
.sf3-text2 {
  font-size: 30rpx;
}
.sdd {
  width: 100%;
}